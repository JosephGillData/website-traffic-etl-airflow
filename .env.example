# Airflow Docker configuration
# Copy this file to .env and fill in your values
# Find your UID with: id -u
AIRFLOW_UID=1000

# Database credentials
POSTGRES_USER=airflow
POSTGRES_PASSWORD=your_postgres_password_here

# Airflow Fernet key (generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())")
AIRFLOW_FERNET_KEY=your_fernet_key_here

# SMTP configuration for email notifications
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password_here
SMTP_MAIL_FROM=your_email@gmail.com

# Email recipient for DAG failure notifications
ALERT_EMAIL=your_email@gmail.com
